{
	"name": "Existing Docker Compose (Extend)",
	"dockerComposeFile": [
		"../docker-compose.yml",
		"docker-compose.yml"
	],
	"service": "web",
	"workspaceFolder": "/usr/src/app",
	"settings": {
		"editor.defaultFormatter": "ms-python.vscode-pylance",
		"python.formatting.provider": "black",
		"[python]": {
			"editor.defaultFormatter": "ms-python.python"
		}
	},
	"extensions": [
		"ms-python.python"
	],
	"runServices": [
		"postgres",
		"redis"
	]
}